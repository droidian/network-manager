"  1 = [30mblack[0m\n"
"  2 = [31mred[0m\n"
"  3 = [32mgreen[0m\n"
"  4 = [33myellow[0m\n"
"  5 = [34mblue[0m\n"
"  6 = [35mmagenta[0m\n"
"  7 = [36mcyan[0m\n"
